{
  "name": "aspnet-core-angular2-starter",
  "version": "1.0.0",
  "description": "ASP.NET Core with Angular2 starter",
  "author": "Krzysztof Koziarski",
  "license": "MIT",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "postinstall": "bower install && gulp rebuild",
    "tsc": "tsc -p ./Frontend",
    "tsc:w": "tsc -w -p ./Frontend",
    "tslint": "tslint -c tslint.json \"./Frontend/app/**/!(*.spec)+(.ts)\" "
  },
  "dependencies": {
    "@angular/common": "~2.2.0",
    "@angular/compiler": "~2.2.0",
    "@angular/core": "~2.2.0",
    "@angular/forms": "~2.2.0",
    "@angular/http": "~2.2.0",
    "@angular/platform-browser": "~2.2.0",
    "@angular/platform-browser-dynamic": "~2.2.0",
    "@angular/router": "~3.2.0",
    "@angular/upgrade": "~2.2.0",
    "angular-in-memory-web-api": "~0.1.15",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.40",
    "zone.js": "^0.6.26"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.35",
    "@types/jasmine": "^2.5.38",
    "@types/node": "^6.0.46",
    "codelyzer": "^2.0.0-beta.3",
    "concurrently": "^3.1.0",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-clean": "^0.3.2",
    "gulp-clean-css": "^2.0.13",
    "gulp-concat": "^2.6.1",
    "gulp-less": "^3.3.0",
    "gulp-plumber": "^1.1.0",
    "gulp-print": "^2.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.1.1",
    "gulp-task-listing": "^1.0.1",
    "gulp-tslint": "^7.0.1",
    "gulp-typescript": "^3.1.3",
    "gulp-util": "^3.0.7",
    "lite-server": "^2.2.2",
    "require-dir": "^0.3.1",
    "run-sequence": "^1.2.2",
    "tslint": "^4.1.1",
    "typescript": "^2.0.10"
  }
